# サーバー設定
NODE_ENV=development
PORT=4000

# データベース設定
DB_HOST=db
DB_PORT=5432
DB_NAME=coincheck_app
DB_USER=postgres
DB_PASSWORD=postgres

# Redis設定
REDIS_HOST=redis
REDIS_PORT=6379

# JWT認証
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=1d

# 暗号化設定
ENCRYPTION_KEY=your_32_character_encryption_key

# CORS設定
CORS_ORIGIN=http://localhost:3000

# Coincheck API設定（開発用ダミー値）
# 実際の値は環境変数か安全なシークレット管理システムで管理すること
COINCHECK_API_KEY=dummy_api_key
COINCHECK_API_SECRET=dummy_api_secret